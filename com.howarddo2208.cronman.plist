<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>com.howarddo208.cronman</string>

        <key>RunAtLoad</key>
        <true/>

        <key>KeepAlive</key>
        <true/>

        <key>ProgramArguments</key>
        <array>
          <string>cronman</string>
        </array>

        <key>StandardOutPath</key>
        <string>/tmp/cronman.log</string>

        <key>StandardErrorPath</key>
        <string>/tmp/observer.cronman.log</string>
    </dict>
</plist>
